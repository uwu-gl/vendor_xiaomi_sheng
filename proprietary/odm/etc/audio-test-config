#capture_top-mic
#enable_top-mic
enable
TX DEC2 MUX:SWR_MIC
TX SMIC MUX2:SWR_MIC5
TX_AIF1_CAP Mixer DEC2:1
ADC2 ChMap:SWRM_TX2_CH2
ADC2 MUX:INP3
TX1 MODE:ADC_LP
ADC2_BCS Disable:1
HDR12 MUX:NO_HDR12
ADC2_MIXER Switch:1
AMIC3_MIXER Switch:1

#disable_top-mic
disable
AMIC3_MIXER Switch:0
ADC2_MIXER Switch:0
HDR12 MUX:NO_HDR12
ADC2_BCS Disable:0
TX1 MODE:ADC_NORMAL
ADC2 MUX:INP2
ADC2 ChMap:ZERO
TX_AIF1_CAP Mixer DEC2:0
TX SMIC MUX2:ZERO
TX DEC2 MUX:MSM_DMIC

#capture_sidetop-mic
#enable_sidetop-mic
enable
TX DEC3 MUX:SWR_MIC
TX SMIC MUX3:SWR_MIC9
TX_AIF1_CAP Mixer DEC3:1
ADC4 MUX:INP5
ADC4 ChMap:SWRM_TX3_CH2
TX3 MODE:ADC_LP
ADC4_MIXER Switch:1
AMIC5_MIXER Switch:1

#disable_sidetop-mic
disable
AMIC5_MIXER Switch:0
ADC4_MIXER Switch:1
TX3 MODE:ADC_NORMAL
ADC4 ChMap:SWRM_TX3_CH2
ADC4 MUX:INP5
TX_AIF1_CAP Mixer DEC3:0
TX SMIC MUX3:ZERO
TX DEC3 MUX:MSM_DMIC

#capture_back-mic
#enable_back-mic
